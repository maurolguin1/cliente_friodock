<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for way bills -->
        <record id="seq_way_bill" model="ir.sequence">
            <field name="name">Way Bill</field>
            <field name="code">csw.way.bill</field>
            <field name="prefix">WB/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for product receptions -->
        <record id="seq_product_reception" model="ir.sequence">
            <field name="name">WH Product Reception</field>
            <field name="code">csw.product.reception</field>
            <field name="prefix">WH.IN/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for product deliveries -->
        <record id="seq_product_delivery" model="ir.sequence">
            <field name="name">WH Product Delivery</field>
            <field name="code">csw.product.delivery</field>
            <field name="prefix">WH.OUT/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for cold storage service -->
        <record id="seq_cold_storage_service" model="ir.sequence">
            <field name="name">Cold Storage Service</field>
            <field name="code">csw.storage.service</field>
            <field name="prefix">CSS/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for stock.quant.package -->
        <record id="seq_quant_package" model="ir.sequence">
            <field name="name">Packages</field>
            <field name="code">stock.quant.package.csw</field>
            <field name="prefix">%(range_y)s/</field>
            <field eval="True" name="use_date_range"/>
            <field name="padding">7</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
